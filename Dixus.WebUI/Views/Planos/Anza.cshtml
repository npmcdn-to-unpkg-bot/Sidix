
@{
    ViewBag.Title = "Mapa Puerta de Anza";
}

<div id="map"></div>

@section scripts{
    <script src="http://cdn.anychart.com/js/latest/anymap.min.js" type="text/javascript"></script>
    <script src="~/scripts/dist/mapa-anza.js"></script>
    @*<script src="Http://cdn.anychart.com/geodata/1.1.0/countries/mexico/mexico.js"></script>*@

}

@section jsCode{
    anychart.onDocumentReady(function() {
    map = anychart.map();

    map.geoData(anychart.maps['anza']);

    //set map container id (div)
    map.container('map');

    //initiate map drawing
    map.draw();
    });

}
